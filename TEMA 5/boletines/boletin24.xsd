<xs:element name="alumnos">
    <xs:complexType>
      <xs:sequence>
      
        <xs:element name="alumno" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
              <xs:sequence>
              
                <xs:element name="nombre" minOccurs="0" maxOccurs="unbounded"/>
                
                <xs:element name="direccion" minOccurs="0" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:sequence>
                    
                      <xs:element name="calle" minOccurs="0" maxOccurs="unbounded"/>
                      <xs:element name="numero" minOccurs="0" maxOccurs="unbounded"/>
                      <xs:element name="ciudad" minOccurs="0" maxOccurs="unbounded"/>
                      <xs:element name="provincia" minOccurs="0" maxOccurs="unbounded"/>
                    
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                
                <xs:element name="telefono" minOccurs="0" maxOccurs="unbounded"/>
                
              </xs:sequence>
            <xs:attribute name="dni" type="xs:integer" use="optional" default="222"/> 
          </xs:complexType>
        </xs:element>
      
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>